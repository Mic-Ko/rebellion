<%- include ("../partials/header") %>
<form onsubmit="return isValidForm();" action="/wynik/<%=wynik._id%>?_method=PUT" method="POST">
    <p class="hidden">Logged Player</p>
	 <select class="hidden" name="wynik[ktoG]" >
		<option value="logged user">current user </option>
	</select>
    <p>Opponent?</p>
    <select name="wynik[kto]" >
<!-- tu trzeba bedzie zrobiÄ‡ petle przez wszystkich urzytkownikow z bazy danych po usernamie -->
	<% user.forEach(function(users){ %>
	<% if (users.username===currentUser.username) { %>
		<option class="hidden"><%= users.username %></option>
	 <% } else { %>
		<option><%= users.username %></option>
	 <% }}); %>
	</select>

    <p>Chose your side?</p>
	<select  class="stR" name="wynik[stronnictwo]" onchange="val()" >
		<option value="Rebels" <%- wynik.stronnictwo=="Rebels" ? "selected" : "" %>  >Rebels</option>
		<option value="Empire" <%- wynik.stronnictwo=="Empire" ? "selected" : "" %>  >Empire</option>
    </select>
    <p>Chose oponennt side?</p>
	<select class="op" name="wynik[opponent]">
		<option value="Rebels" class="opR" <%- wynik.opponent=="Rebels" ? "selected" : "" %>  >Rebels</option>
		<option value="Empire" class="opE" <%- wynik.opponent=="Empire" ? "selected" : "" %>  >Empire</option>
	</select>


    <p>Today game win?</p>
	<select  name="wynik[wygral]"  >
