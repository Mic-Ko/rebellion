<%- include ("../partials/header") %>




<div class="row">
    <div class="col-sm-6 col-md-4">
      <div class="thumbnail">
        <img src="<%=user.avatar%>" alt="user profile image">
        <div class="caption">
          <h3><%= user.nick %></h3>
